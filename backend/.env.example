# Jumpseller API Configuration
# Copy this file to .env and fill in your actual credentials

# Your Jumpseller store login (acts as username)
JUMPSELLER_LOGIN=your_store_login_here

# Your Jumpseller auth token (acts as password)
JUMPSELLER_AUTH_TOKEN=your_auth_token_here

# Jumpseller API base URL (usually doesn't change)
JUMPSELLER_API_BASE_URL=https://api.jumpseller.com/v1

# Optional: API timeout in seconds
JUMPSELLER_API_TIMEOUT=30